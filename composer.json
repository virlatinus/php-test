{
    "name": "virlatinus/php-test",
    "description": "Test PHP Project",
    "type": "project",
    "minimum-stability": "stable",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Virlatinus\\PhpTest\\": "src/",
            "tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Werner Garc√≠a",
            "email": "werner.garcia@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "start": "@php index.php",
        "test": "@php vendor/bin/phpunit tests",
        "serve": "@php -S 0.0.0.0:8080 index.php"
